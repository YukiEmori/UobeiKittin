<Window x:Class="UobeiKittin.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UobeiKittin"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="Beige" >
    <Window.Resources>
        <local:SushiOrderDBDataSet x:Key="sushiOrderDBDataSet"/>
        <CollectionViewSource x:Key="testViewSource" Source="{Binding test, Source={StaticResource sushiOrderDBDataSet}}"/>
    </Window.Resources>

    <Grid DataContext="{StaticResource testViewSource}">
        <Frame Name="frame" NavigationUIVisibility="Hidden" Source="{Binding}"/>

    </Grid>

</Window>
